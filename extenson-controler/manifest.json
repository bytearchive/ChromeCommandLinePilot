{
  "name": "Chrome Command Line Tab Controller",
  "version": "1.2",
  "manifest_version": 2,
  "description": "Websocket client to controll tabs remotely",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "default_locale": "en",
  
  "background": {
    "persistent": true,
    "page": "index.html"
  },
  "permissions": ["tabs","experimental", "pageCapture", "<all_urls>"]
}
